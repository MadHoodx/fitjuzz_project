{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "fitjuzz",
    "slug": "fitjuzz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fitjuzzcompany.fitjuzz"
    },
    "web": {
      
    },
    "extra": {
      "eas": {
        "projectId": "be343713-6a85-47b6-9aeb-a8c816e43405"
      }
    },
    "scheme": "fitjuzz"
  }
}
